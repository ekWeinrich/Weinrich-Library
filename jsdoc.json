{
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    },
    "theme_opts": {
        
    },    
    "plugins": ["plugins/markdown"],
    "source": {
        "include": [
        "src",
        "Tutorials"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "encoding": "utf8",
        "template": "./node_modules/clean-jsdoc-theme",
        "destination": "./docs/",
        "readme": "./README.md",
        "tutorials": "./Tutorials",
        "recurse": true,
        "verbose": true,
        "base_url": "https://ekweinrich.github.io/Weinrich-Library//",
        "theme_opts": {
            "default_theme": "dark",
            "homepageTitle": "ELO Library - Weinrich",
            "menu": [
                {
                    "title": "Google",
                    "link": "https://www.google.com/",
                    "id": "google"
                }
            ]
        }
    }
}